{
    "expo": {
        "name": "ShapeShift",
        "slug": "ShapeShift",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/splash_screen.jpg",
            "resizeMode": "cover",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/app_icon.svg",
                "backgroundColor": "#ffffff"
            }
        },
        "web": {
            "favicon": "./assets/app_icon.svg"
        }
    }
}
